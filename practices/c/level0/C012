#include <stdio.h>
main()
{
printf("hello world,I am Guohu!");
}


#include<stdio.h>
main()
{
	int a;
	printf("输入a的值：");
	scanf("%d",&a);
	
	if(a>0)
	printf("a为正数");
	else if(a==0)
	printf("a为0");
	else 
	printf("a为负数");
	printf("\n");
	
	if(a%2==0)
	printf("a为偶数");
	else 
	printf("a为奇数"); 
	printf("\n");
	
	if(a%13==0)
	printf("a为13倍数");
	else
	printf("a不为13倍数");
}


#include <stdio.h>
int main()
{
	int a;
	printf("输入年份：");
	scanf("%d",&a);
	if(a%400==0||a%100!=0&&a%4==0)
	printf("a为闰年");
	else
	printf("a不为闰年");
}


#include <stdio.h>
void main()
{
	int a,b,c;
	printf("输入三个数：");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b)
	{
		if(a>c)
		printf("最大的为%d",a);
		else
		printf("最大的为%d",c);
	}
	else
	{
		if(b<c)
		printf("最大的为%d",c);
		else
		printf("最大的为%d",b);
	}
}


#include<stdio.h>
int main()
{
	int m,n,a,i;
	m=1;
	
	printf("n的值为:");
	scanf("%d",&n);
	printf("输入n个数");
	
	for(i=1;i<=n;i++)
	{
	scanf("%d",&a);
	printf("\n");
	if(a>m)
	{
		m=a;
		
	}
	}
	printf("最大的数为%d",m);
 } 	
 
 
